                    case 'radio-intelejen':
                        if($("#"+id).attr('checked')=="checked"){
                            stat_view.intelegent = true;
                            if(markerPoi.length>0){
                                for(var i in markerPoi){
                                    markerPoi[i].setOpacity(1);
                                }
