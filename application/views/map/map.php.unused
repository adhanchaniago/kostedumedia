                        console.log('all');
                    }else if(stat_view.kri==true && stat_view.track==false){
                        socket.emit('reqSubscribe',{mode:'kri'});
                        socket.emit('reqUnsubscribe',{mode:'all'});
                        socket.emit('reqUnsubscribe',{mode:'track'});
