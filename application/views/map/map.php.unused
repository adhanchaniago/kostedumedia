                        }else if(stat_view.kri==false && (markerKRI[i]['kri']!=undefined || markerKRI[i]['kri']!=null )){
                            markerKRI[i]['kri'].setOpacity(0);
                            markerKRIdata[i]['kri'].view=false;
                        }
                    }
